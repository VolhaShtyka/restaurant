<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        <hibernate-mapping package="com.shtyka.entity" auto-import="false" catalog="restoraut">
    <class name="User" table="client">
        <id name="id" type="int" column="client_id">
        <generator class="increment"/>
        </id>
        <property name="name" column="client_name" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="roleId" column="role_id" type="int"/>
        <property name="tableNumber" column="table_number" type="int"/>

    </class>
 </hibernate-mapping>