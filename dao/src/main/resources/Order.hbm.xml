<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        <hibernate-mapping package="com.shtyka.entity" auto-import="false" catalog="restoraut">
    <class name="Order" table="order">
        <id name="orderId" type="int" column="order_id">
        <generator class="increment"/>
        </id>
        <property name="clientId" column="client_id" type="int"/>
        <property name="statusOrder" column="status_order" type="string"/>
        <property name="menuId" column="menu_id" type="int"/>
    </class>
 </hibernate-mapping>